    "# Shelgir Aziz\n",
    "# Question 4\n",
    "\n",
    "def collatz(x):\n",
    "    while x != 1:\n",
    "        print(x, end=' ')\n",
    "        if x % 2 == 0:\n",
    "            x = x // 2\n",
    "        else:\n",
    "            x = x * 3 + 1\n",
    "    print(x)"
